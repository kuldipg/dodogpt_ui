            } else {
                scrollShadow.removeClass("scroll-shadow__left");
            }

            if (scrollLeft + offsetWidth < scrollWidth) {
                scrollShadow.addClass("scroll-shadow__right");
            } else {
                scrollShadow.removeClass("scroll-shadow__right");
            }
        } else {
            scrollShadow.removeClass("scroll-shadow__left");
            scrollShadow.removeClass("scroll-shadow__right");
        }
